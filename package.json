{
  "name": "@vpmedia/phaser-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/phaser package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/phaser-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/phaser-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/phaser-example.git"
  },
  "type": "module",
  "main": "src/index.html",
  "scripts": {
    "asset": "phixify all --config-file ./.phixify.json",
    "start": "vite --open",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview --open",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "stylelint": "stylelint \"**/*.css\" --ignore-path .gitignore",
    "typecheck": "tsc",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,md,css}\"",
    "test": "echo \"Warning: no test specified\" && exit 0",
    "lefthook:install": "lefthook install",
    "lefthook:uninstall": "lefthook uninstall"
  },
  "dependencies": {
    "@vpmedia/phaser": "^1.76.0",
    "fontfaceobserver": "^2.3.0",
    "spectorjs": "^0.9.30"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@types/fontfaceobserver": "^2.1.3",
    "@types/jest": "^29.5.14",
    "@vpmedia/phixify": "^2.0.1",
    "eslint": "^9.20.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-unicorn": "^57.0.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lefthook": "^1.10.10",
    "prettier": "^3.5.1",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.1"
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not op_mini all"
  ]
}
