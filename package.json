{
  "name": "@vpmedia/phaser-example",
  "version": "1.0.0",
  "description": "Example for the @vpmedia/phaser package",
  "author": "Andras Csizmadia <andras@vpmedia.hu> (www.vpmedia.hu)",
  "license": "MIT",
  "homepage": "https://github.com/vpmedia/phaser-example#readme",
  "bugs": {
    "url": "https://github.com/vpmedia/phaser-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpmedia/phaser-example.git"
  },
  "browserslist": "> 1%, not dead",
  "source": "src/index.html",
  "scripts": {
    "asset": "phixify all --config-file ./.phixify.json",
    "prestart": "rm -rf dist && mkdir -p dist && cp -r public/* dist/",
    "start": "parcel --open",
    "prebuild": "rm -rf dist",
    "build": "parcel build",
    "postbuild": "cp -r public/* dist/",
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --write \"**/*.{js,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@vpmedia/phaser": "^1.1.4",
    "spectorjs": "^0.9.27"
  },
  "devDependencies": {
    "@vpmedia/phixify": "^1.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "parcel": "^2.8.0",
    "prettier": "^2.8.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  }
}
